<launch>
       <remap from="/arduino_mega/bno/imu" to="/bno/imu"/>
       <remap from="/arduino_mega/bno/imuStat" to="/bno/imuStat"/>
       <node ns="arduino_mega" pkg="rosserial_python" type="serial_node.py" name="serial_node_mega" output="screen">
       <param name="port" value="/dev/ttyACM0"/>
       <param name="baud" value="57600"/>
       </node>

        <remap from="/arduino_teensy/raw_vel" to="/raw_vel"/>
        <remap from="/arduino_teensy/cmd_vel" to="/cmd_vel"/>
        <remap from="/arduino_teensy/pid" to="/pid"/>
        <node ns="arduino_teensy" pkg="rosserial_python" type="serial_node.py" name="serial_node_teensy" output="screen">
        <param name="port" value="/dev/ttyACM1"/>
        <param name="baud" value="57600"/>
        </node>

        <arg name="model" default="$(find rikirobot)/urdf/zee_robot.urdf"/>
        <arg name="gui" default="true" />
        <arg name="rvizconfig" default="$(find rikirobot)/rviz/zee_robot.rviz" />

        <param name="robot_description" command="cat $(find rikirobot)/urdf/zee_robot.urdf" />
	<param name="use_gui" value="$(arg gui)"/>

        <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher" />
        <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher" />

	<node name="rviz" pkg="rviz" type="rviz" args="-d $(arg rvizconfig)" required="true" />
</launch>
